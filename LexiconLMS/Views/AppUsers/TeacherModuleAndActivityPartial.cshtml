@model TeacherViewModel

<div class="container cont-section" id="cont-modules-and-activities">
    <!--Modules and Activities-->
    <div class="row row-modules">
        <div id="col-modules" class="col-sm-3 text-center">
            <h2 style="margin: 5px 25px 20px 0px">Modules<a asp-controller="Modules" asp-action="Create" asp-route-id="@Model.Current.Course.Id"><img style="margin: 0px 0px 5px 5px" id="btn-create" src="/icons/create.svg" alt="Alternate Text" /></a></h2>
            <partial name="TeacherModulePartial" model="@Model.ModuleList" />
        </div>
        <div id="col-activities" class="col-sm-9">
            <partial name="TeacherActivityPartial" model="@Model.ActivityList" />
        </div>
    </div>
</div>